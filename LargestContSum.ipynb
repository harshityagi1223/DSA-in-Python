   "source": [
    "from typing import List\n",
    "def larg_cont_sum(nums : List[int]):\n",
    "    max_sum = curr_sum = nums[0]\n",
    "    for num in nums[1:]:\n",
    "        curr_sum = max(curr_sum+num,num)\n",
    "        max_sum = max(max_sum,curr_sum)\n",
    "    return max_sum"
   ]
